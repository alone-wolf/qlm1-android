<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qlm1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Qlm1">
        <activity
            android:name=".activitys.SubSettingsActivity"
            android:exported="true"
            android:label="@string/title_activity_sub_settings"
            android:theme="@style/Theme.Qlm1.NoActionBar" />
        <activity
            android:name=".activitys.MainSearchActivity"
            android:exported="true"
            android:label="@string/title_activity_main_search"
            android:theme="@style/Theme.Qlm1.NoActionBar" />
        <activity
            android:name=".activitys.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Qlm1.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>